<html>
    <head>
        <link rel="stylesheet" type="text/css" href="styles.css">
    </head>
<body>
    <div>
        <h1>Here are the awesome quotes!</h1>
        <ul>
                <% for(var i=0 ; i < allQuotes.length; i++){ %>
                <h3><%= allQuotes[i].quotes %></h3>
                <p>- <%= allQuotes[i].name %> at <%= allQuotes[i].createdAt %></p>  
                <hr>
                <% } %>

                <% if(typeof(errors) != 'undefined' ) { %>
                    <% for (var x in errors) { %>
                        <h3><%= errors[x].message %></h3>
                    <% } %>
                <% } %>

        </ul>
    </div>
</body>
</html>
